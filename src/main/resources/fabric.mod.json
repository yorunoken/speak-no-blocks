{
  "schemaVersion": 1,
  "id": "speak-no-blocks",
  "version": "${version}",
  "name": "Speak No Blocks",
  "description": "Voice controlled block breaking!",
  "authors": [
    "Yorunoken"
  ],
  "contact": {
    "sources": "https://github.com/Yorunoken/speak-no-blocks"
  },
  "license": "MIT",
  "icon": "assets/speak-no-blocks/icon.png",
  "environment": "*",
  "entrypoints": {
