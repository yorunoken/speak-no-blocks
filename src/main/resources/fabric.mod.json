      "com.yorunoken.speakNoBlocks.client.SpeakNoBlocksDataGenerator"
    ],
    "client": [
      "com.yorunoken.speakNoBlocks.client.SpeakNoBlocksClient"
    ],
    "main": [
      "com.yorunoken.speakNoBlocks.SpeakNoBlocks"
    ],
    "modmenu": [
      "com.yorunoken.speakNoBlocks.client.ModMenuIntegration"
